generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Classe {
  id  Int  @id @default(autoincrement())
  Classe Int
  Sezione String @db.Char(1)
  Indirizzo String @db.VarChar(5)
  AnnoScolastico Int 
}
